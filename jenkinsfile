pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }    

    stages {
        stage('build') {
            steps {
                sh 'mvn clean'
                sh 'mvn install'
                sh 'mvn package'
                sleep 5
            }
        }
        stage('test') {
            steps {
                sh 'mvn test'
                sleep 5

            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
                sleep 5
            }
        }
        stage('Docker') {
            steps {
                sh 'docker image'
                sleep 5
            }
        }
    }
}                         
