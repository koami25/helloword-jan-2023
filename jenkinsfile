pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }

    stages {
        stage('build') {
            steps {
                sh 'maven clean'
                sh  'maven install'
                sh  'maven package'
                sleep 10
            }
        }
        stage('test') {
            steps {
                sh 'maven test'
                sleep 10
            }
        }
        stage('Deploy') {
            steps {
                echo 'deployment step'

            }
        }
        stage('docker') {
            steps {
                echo 'docker image step'
            }
        }
        
    }
}